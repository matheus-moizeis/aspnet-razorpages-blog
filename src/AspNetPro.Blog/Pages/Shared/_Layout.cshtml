@{
    ViewData["AppName"] = "ASP.NET PRO BLOG";
}
<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <meta name="description" content="" />
    <meta name="author" content="" />
    <title>@ViewData["AppName"]</title>
    <!-- Favicon-->
    <link rel="icon" type="image/x-icon" href="~/assets/favicon.ico" />
    <!-- Core theme CSS (includes Bootstrap)-->
    <link href="~/css/styles.css" rel="stylesheet" />
    @RenderSection("Css", required: false)
</head>
<body>
    <partial name="Partials/_Header" />
    @* @await Html.PartialAsync("Partials/_Header")*@

    @RenderSection("Tagline", required: false)

    <!-- Page content-->
    <div class="container @ViewData["PageContentClass"]">
        <div class="row">
            <div class="col-lg-8">
                @RenderBody()
            </div>
            <!-- Side widgets-->
            <div class="col-lg-4">
                <partial name="Partials/_SideBar" />
            </div>
        </div>
    </div>

    <partial name="Partials/_Footer" />
    <script src="~/lib/jquery/jquery.min.js"></script>
    <!-- Bootstrap core JS-->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js"></script>
    <!-- Core theme JS-->
    <script src="~/js/scripts.js"></script>
    @RenderSection("Scripts", required: false);
</body>
</html>